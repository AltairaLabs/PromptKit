{
  "name": "promptarena-action",
  "version": "1.0.0",
  "description": "GitHub Action for running PromptArena tests",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build lib/main.js -o dist --source-map --license licenses.txt",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "all": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AltairaLabs/PromptKit.git"
  },
  "keywords": [
    "github-action",
    "promptarena",
    "llm",
    "testing",
    "prompt-testing"
  ],
  "author": "AltairaLabs",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vercel/ncc": "^0.38.1",
    "typescript": "^5.3.2"
  }
}
