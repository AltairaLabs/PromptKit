{
  "name": "packc-action",
  "version": "1.1.10",
  "description": "GitHub Action for compiling and publishing prompt packs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build lib/main.js -o dist --source-map --license licenses.txt",
    "lint": "eslint src/**/*.ts",
    "test": "jest --coverage",
    "test:ci": "jest --coverage --ci --reporters=default --reporters=jest-junit",
    "all": "npm run lint && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AltairaLabs/PromptKit.git"
  },
  "keywords": [
    "github-action",
    "packc",
    "prompt-pack",
    "oci",
    "registry"
  ],
  "author": "AltairaLabs",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.55.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  }
}
