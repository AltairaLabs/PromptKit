{
  "name": "@altairalabs/packc",
  "version": "1.1.0",
  "type": "module",
  "description": "PromptKit Pack Compiler - Compile and validate prompt packs for LLM applications",
  "bin": {
    "packc": "./bin/packc.js"
  },
  "scripts": {
    "postinstall": "node postinstall.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --ci",
    "test:binary": "node bin/packc.js version",
    "lint": "eslint *.js bin/*.js lib/*.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "^9.39.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0"
  },
  "keywords": [
    "llm",
    "prompt",
    "compiler",
    "validation",
    "ai",
    "prompt-engineering",
    "pack",
    "openai",
    "anthropic",
    "gemini"
  ],
  "author": "Altaira Labs",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AltairaLabs/PromptKit.git",
    "directory": "npm/packc"
  },
  "bugs": {
    "url": "https://github.com/AltairaLabs/PromptKit/issues"
  },
  "homepage": "https://github.com/AltairaLabs/PromptKit#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "files": [
    "bin/",
    "lib/",
    "postinstall.js",
    "README.md"
  ]
}
