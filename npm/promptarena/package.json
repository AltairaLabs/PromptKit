{
  "name": "@altairalabs/promptarena",
  "version": "1.1.0",
  "type": "module",
  "description": "PromptKit Arena - Multi-turn conversation simulation and testing tool for LLM applications",
  "bin": {
    "promptarena": "./bin/promptarena.js"
  },
  "scripts": {
    "postinstall": "node postinstall.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --ci",
    "test:binary": "node bin/promptarena.js --version",
    "test:init": "node test-init.js",
    "lint": "eslint *.js bin/*.js"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "jest": "^30.2.0"
  },
  "keywords": [
    "llm",
    "testing",
    "prompt",
    "ai",
    "conversation",
    "evaluation",
    "chatbot",
    "openai",
    "anthropic",
    "gemini",
    "quality-assurance",
    "test-automation"
  ],
  "author": "Altaira Labs",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AltairaLabs/PromptKit.git",
    "directory": "npm/promptarena"
  },
  "bugs": {
    "url": "https://github.com/AltairaLabs/PromptKit/issues"
  },
  "homepage": "https://github.com/AltairaLabs/PromptKit#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "files": [
    "bin/",
    "lib/",
    "postinstall.js",
    "README.md",
    "schemas/",
    "schema-map.json"
  ],
  "exports": {
    "./schemas/*": "./schemas/*.json",
    "./schema-map": "./schema-map.json"
  }
}
